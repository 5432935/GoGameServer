#server启动
pm2 start consul --name consul -- agent -server -bootstrap-expect 2 -data-dir /tmp/consul -node=n1 -bind=10.0.0.10 -ui -client 0.0.0.0
pm2 start consul --name consul -- agent -server -bootstrap-expect 2 -data-dir /tmp/consul -node=n2 -bind=10.0.0.17

#客户端启动(game)
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n3 -bind=10.0.0.15
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n4 -bind=10.0.0.16
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n5 -bind=10.0.0.3

#客户端启动(http)
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n6 -bind=10.0.0.68
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n7 -bind=10.0.0.70
pm2 start consul --name consul -- agent -data-dir /tmp/consul -node=n8 -bind=10.0.0.64

#加入集群
consul join 10.0.0.10



